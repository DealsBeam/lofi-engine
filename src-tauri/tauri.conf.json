{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "Lofi Engine",
  "mainBinaryName": "Lofi Engine",
  "version": "1.0.2",
  "identifier": "com.lofi-engine.dev",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "macOSPrivateApi": false,
    "windows": [
      {
        "fullscreen": false,
        "title": "Lofi Engine",
        "width": 980,
        "height": 580,
        "minWidth": 930,
        "minHeight": 530,
        "decorations": true,
        "transparent": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "trafficLightPosition": {
          "x": -100,
          "y": -100
        }
      }
    ],
    "security": {
      "csp": null
    }
  }
}